set terminal png
set output 'result.png'
set timefmt "%d:%H:%M:%S"
set format x "%M:%S"
set xdata time
set xlabel "Time (M:S)"
set ylabel "#Containers"
set yrange [0:850]
set style data line
plot 'result_benchmark.dat' every ::2 using 1:6 t '#dead', '' every ::2 using 1:7 t '#alive', '' every ::2 using 1:8 t '#total'
